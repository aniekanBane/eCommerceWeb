{
  "baseUrls": {
    "apiBase": "http://ecommerceweb-public-api/",
    "webBase": "http://ecommerceweb-webapp/"
  },
  "DatabaseSettings": {
    "StoreDb": {
      "ConnectionString": "Host=ecommerceweb-db;Database=dev_ecommerceweb_db;Username=postgres;Password=secure123!;Pooling=true;Include Error Detail=true"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Seq": {
      "IsEnabled": true,
      "ServerUrl": "http://ecommerceweb-seq-log-server",
      "ApiKey": null
    }
  },
  "StorageOptions": {
    "Provider": "Azure",
    "Azure": {
      "ConnectionString": "UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://ecommerceweb-storage",
      "ContainerName": "ecommerceweb-assets"
    }
  }
}
